{
  "version": 2,
  "public": true,
  "name": "fun-life-app",
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.backthennow.com/",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "./dist/fun_life_app/browser/**"
    }
  }
}
